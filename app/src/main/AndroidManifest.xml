<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.completalo_levi">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Completalo_levi">
        <activity
            android:name="org.iesmurgi.completalo_levi.Winner"
            android:exported="false" />
        <activity
            android:name="org.iesmurgi.completalo_levi.GameField_levi"
            android:exported="false" />
        <activity
            android:name="org.iesmurgi.completalo_levi.About_levi"
            android:exported="false"
            android:label="@string/About" />
        <activity
            android:name="org.iesmurgi.completalo_levi.HowTo_levi"
            android:exported="false"
            android:label="@string/HowTo" />
        <activity
            android:name="org.iesmurgi.completalo_levi.MainActivity_levi"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>